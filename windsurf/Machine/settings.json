{
    "windsurf.autocompleteSpeed": "fast",
    "windsurf.autoExecutionPolicy": "off",
    "windsurf.chatFontSize": "default",
    "windsurf.rememberLastModelSelection": true,
    "windsurf.openRecentConversation": true,
    "windsurf.explainAndFixInCurrentConversation": true,
    "editor.tabSize": 2,
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#415c6d"
    },
    "[jsonl]": {
        "editor.rulers": [
            120
        ]
    },
    "[javascript]": {
        "editor.rulers": [
            120
        ]
    },
    "[vue]": {
        "editor.rulers": [
            120
        ]
    },
    "[ruby]": {
        "editor.rulers": [
            80
        ]
    },
    "files.associations": {
        "*.tsv": "csv"
    },
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
    "files.trimTrailingWhitespace": true,
    "remote.autoForwardPortsSource": "hybrid",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "rubyLsp.rubyVersionManager": {
        "identifier": "chruby"
    },
    "rubyLsp.bundleGemfile": "/workspaces/monorama/.devcontainer/engineering/tools/lsp/Gemfile",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "eslint.validate": [
        "javascript",
        "vue"
    ],
    "eslint.useESLintClass": true,
}
